*/

RULE debug
  FORALL _ O
  WHERE println(O)
;

RULE transformation
  FORALL M^Transformation st {
	     name: tn;
         }
  MAKE   R^Transformation tt {
	    name: tn;
         }
  LINKING link WITH src=st, tgt=tt
;

