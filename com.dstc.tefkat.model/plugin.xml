<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="com.dstc.tefkat.model"
   name="%pluginName"
   version="2.0.9.02"
   provider-name="%providerName"
   class="com.dstc.tefkat.model.provider.TefkatModelEditPlugin$Implementation">

   <runtime>
      <library name="TefkatModel.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.core.runtime.compatibility"/>
      <import plugin="org.eclipse.emf.ecore" export="true"/>
      <import plugin="org.eclipse.emf.edit"/>
      <import plugin="org.eclipse.emf.ecore.xmi"/>
      <import plugin="org.eclipse.emf.edit.ui"/>
      <import plugin="org.eclipse.emf.ecore.edit"/>
      <import plugin="org.eclipse.emf.ecore.editor"/>
      <import plugin="org.eclipse.emf.common"/>
      <import plugin="org.eclipse.core.resources"/>
   </requires>


   <extension
         point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://www.dstc.edu.au:8080/qvt/models/TefkatModel-2.2.ecore"
            class="com.dstc.tefkat.model.TefkatPackage">
      </package>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="%_UI_Wizard_category"
            id="org.eclipse.emf.ecore.Wizard.category.ID">
      </category>
      <wizard
            name="%_UI_TefkatModelWizard_label"
            icon="icons/full/obj16/TefkatModelFile.gif"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            class="com.dstc.tefkat.model.presentation.TefkatModelWizard"
            id="com.dstc.tefkat.model.presentation.TefkatModelWizardID">
         <description>
            %_UI_TefkatModelWizard_description
         </description>
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="%_UI_TefkatEditor_label"
            icon="icons/full/obj16/TefkatModelFile.gif"
            extensions="tefkat"
            contributorClass="com.dstc.tefkat.model.presentation.TefkatActionBarContributor"
            class="com.dstc.tefkat.model.presentation.TefkatEditor"
            id="com.dstc.tefkat.model.presentation.TefkatEditorID">
      </editor>
   </extension>

</plugin>
