<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="com.dstc.tefkat.engine"
   name="%pluginName"
   version="2.1.0.1"
   provider-name="%providerName">

   <runtime>
      <library name="tefkat_engine.jar">
         <export name="*"/>
      </library>
      <library name="jgraph.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.emf.ecore" export="true"/>
      <import plugin="org.eclipse.emf.ecore.xmi" export="true"/>
      <import plugin="com.dstc.tefkat.config" export="true"/>
      <import plugin="com.dstc.tefkat.model" export="true"/>
      <import plugin="com.dstc.tefkat.model.parser" export="true"/>
   </requires>

   <extension
         point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http:///com/dstc/tefkat/TefkatTrace.ecore"
            class="com.dstc.tefkat.engine.trace.TracePackage">
      </package>
   </extension>

</plugin>
